{
    "version": 2,
    "name": "design",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node@^1.8.1"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/main.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}